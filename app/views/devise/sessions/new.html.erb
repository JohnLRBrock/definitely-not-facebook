<header id="login-header">
  <div class="wrapper">
    <%= link_to 'definitely not facebook', root_path, title: 'Go to Definitely Not Facebook Home', class: 'definitely-not-logo' %>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="field">
        <%= f.label :email_or_Phone %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    <% end %>
  </div>
  <%# <%= render "devise/shared/links" %>
</header>
